<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.image.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox fillWidth="false" maxHeight="500.0" maxWidth="300.0" minHeight="150.0" minWidth="300.0" prefHeight="150.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.MessageContainerModalController">
   <children>
      <HBox maxWidth="300.0" minWidth="300.0" prefWidth="300.0">
         <children>
            <ImageView fx:id="imgMessageContainerModal" fitHeight="50.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true" HBox.hgrow="NEVER">
               <viewport>
                  <Rectangle2D height="50.0" width="50.0" />
               </viewport>
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </ImageView>
            <Label fx:id="labelMessageContainerTitle">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
            </Label>
         </children>
      </HBox>
      <Label fx:id="labelMessageContainerText" maxWidth="300.0" minWidth="300.0" prefWidth="300.0">
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </Label>
      <FlowPane alignment="TOP_RIGHT" maxWidth="300.0" minWidth="300.0" prefWidth="300.0">
         <children>
            <Button fx:id="buttonMessageContainerOk" alignment="CENTER" maxHeight="22.0" maxWidth="50.0" minHeight="22.0" minWidth="50.0" mnemonicParsing="false" prefHeight="22.0" prefWidth="50.0" text="OK">
               <FlowPane.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </FlowPane.margin>
            </Button>
         </children>
      </FlowPane>
   </children>
</VBox>
